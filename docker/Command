./mc-conf.sh create-user -H=${MC_DATA} -n=admin -p=p@ssw0rd -r=admin

## Creation of docker build mancenter
docker build -t mc-built-in-user .
docker run -it --rm -p 8080:8080 --name mc_server mc-built-in-user


## Creation of docker build hazelcast
docker run --rm --name member1 hazelcast/hazelcast:3.12.6

docker build -t hazelcast-custom .
docker run -it --rm -p 5701:5701 --name member1 --link mc_server:mc_server hazelcast-custom 


docker run -it --rm -p 5701:5701 --name member1 hazelcast-custom 